<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context = "http://www.springframework.org/schema/context"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config/>

    <bean id="triangle" class="com.github.aman.Triangle">
<!--        <property name="p1" ref="point1"/>-->
<!--        <property name="p2" ref="point2"/>-->
<!--        <property name="p3" ref="point3"/>-->
    </bean>

    <bean id="circle" class="com.github.aman.circle">
<!--        <property name="center" ref="point1"/>-->
    </bean>
    <bean id="point1" class="com.github.aman.Point">
<!--        <qualifier type="CircleRelated"/>-->
        <property name="x" value="0"/>
        <property name="y" value="0"/>
    </bean>

    <bean id="point2" class="com.github.aman.Point">
        <property name="x" value="0"/>
        <property name="y" value="20"/>
    </bean>

    <bean id="point3" class="com.github.aman.Point">
        <property name="x" value="-20"/>
        <property name="y" value="0"/>
    </bean>

    <bean class="org.springframework.beans.factory.annotation.AutowiredAnnotationBeanPostProcessor"/>
</beans>